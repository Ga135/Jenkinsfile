pipeline{
    agent any

    stages{
        stage('Checkout'){
            steps{
                withCredentials([usernamePassword(credentialsId:'test_up', usernameVariable:'USER', passwordVariable: 'Password')])
                {
                sh 'echo $USER $Password'
                }

                withCredentials([string(credentialsId:'str',variable:'Secret_text')])
                {
                sh 'echo $str $Secret_text'
                }

                withCredentials([file(credentialsId:'fl',variable:'File_path')])
                {
                sh 'cat $File_path'
                }

                withCredentials([sshUserPrivateKey(credentialsId:'spk',usernameVariable:'User', keyFileVariable:'SSH_KEY')])
                {
                sh 'echo $spk $User $SSH_KEY'

                }
            }
        }
    }
    

}